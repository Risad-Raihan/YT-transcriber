# API Keys - Copy this file to .env and fill in your actual keys
# Never commit the .env file to version control


# Mathpix OCR API (for extracting equations and diagrams)
MATHPIX_APP_ID=your_mathpix_app_id_here
MATHPIX_APP_KEY=your_mathpix_app_key_here

# Google Gemini API (for vision analysis)
GOOGLE_API_KEY=your_google_api_key_here

# Hugging Face Token (optional, for gated models)
HUGGINGFACE_TOKEN=your_huggingface_token_here

